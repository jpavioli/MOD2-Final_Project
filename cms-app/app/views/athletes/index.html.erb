<h1>All Athletes</h1>
<p>
  <% @athletes.each do |athlete|%>
    <li>
        <%= link_to athlete.name, athlete_path(athlete) %> | <%= link_to athlete.team.name, team_path(athlete.team) %> | <%= link_to athlete.team.competition.name, competition_path(athlete.team.competition) %>
    </li>
  <%end%>
</p>
