<% if flash[:error] %>
<% flash[:error].each do |e| %>
<p><%= e %></p>
<%end%>
<%end%>

<%= form_for @event do |f| %>
<%= f.label "Title:" %>
<%= f.text_field :title %>
<br />
<%= f.label "Description:" %>
<%= f.text_field :description %>
<br/>
<%= f.label "Score Type:" %>
<%= f.text_field :score_type %>
<%= f.submit %>
<% end %> 